- content_for :title do
	Login

#login_form
	%h1 Login
	%h2#notice= notice
	%form{:method => 'post', :action => users_login_go_path}
		%input{:name => 'username', :type => 'text', :placeholder => 'Username' }
		%input{:name => 'password', :type => 'password', :placeholder => 'Password' }
		%input{:type => 'submit'}